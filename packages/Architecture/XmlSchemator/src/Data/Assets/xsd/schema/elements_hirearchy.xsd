- <xs:element name="Comprobante">
    - <xs:element name="Emisor">      <!-- belongs to Comprobante, 1 and only one must be defined -->
    - <xs:element name="Receptor">
    - <xs:element name="Addenda" minOccurs="0">   <!-- belongs to Comprobante, can be omitted or 1 and only one can be defined -->
    - <xs:element name="Complemento" minOccurs="0" maxOccurs="unbounded"> <!-- belongs to Comprobante, can be omitted or an unlimited amount of them can be defined -->

    - <xs:element name="CfdiRelacionados" minOccurs="0">  <!-- belongs to Comprobante, can be omitted or 1 and only one can be defined -->
        - <xs:element name="CfdiRelacionado" minOccurs="1" maxOccurs="unbounded"> <!-- belongs to CfdiRelacionados, at least 1 or an unlimited amount of them must be defined -->

    - <xs:element name="Conceptos"> <!-- belongs to Comprobante, 1 and only one must be defined -->
        - <xs:element name="Concepto" maxOccurs="unbounded"> <!-- belongs to Conceptos, at least 1 or an unlimited amount of them must be defined -->
            - <xs:element name="CuentaPredial" minOccurs="0"> <!-- belongs to Concepto, can be omitted or 1 and only one can be defined -->
            - <xs:element name="ComplementoConcepto" minOccurs="0"> <!-- belongs to Concepto, can be omitted or 1 and only one can be defined -->
            - <xs:element name="InformacionAduanera" minOccurs="0" maxOccurs="unbounded"> <!-- belongs to Concepto, can be omitted or an unlimited amount of them can be defined -->
            - <xs:element name="Parte" minOccurs="0" maxOccurs="unbounded"> <!-- belongs to Concepto, can be omitted or an unlimited amount of them can be defined -->
                - <xs:element name="InformacionAduanera" minOccurs="0" maxOccurs="unbounded"> <!-- belongs to Parte, can be omitted or an unlimited amount of them can be defined -->
            <xs:element name="Impuestos" minOccurs="0"> <!-- belongs to Concepto, can be omitted or 1 and only one can be defined -->
                - <xs:element name="Traslados" minOccurs="0"> <!-- belongs to Impuestos, can be omitted or 1 and only one can be defined -->
                    - <xs:element name="Traslado" maxOccurs="unbounded"> <!-- belongs to Traslados, at least 1 or an unlimited amount of them must be defined -->
                - <xs:element name="Retenciones" minOccurs="0"> <!-- belongs to Impuestos, can be omitted or 1 and only one can be defined -->
                    - <xs:element name="Retencion" maxOccurs="unbounded"> <!-- belongs to Retenciones, at least 1 or an unlimited amount of them must be defined -->

    - <xs:element name="Impuestos" minOccurs="0"> <!-- belongs to Comprobante, can be omitted or 1 and only one can be defined -->
        - <xs:element name="Retenciones" minOccurs="0"> <!-- belongs to Impuestos, can be omitted or 1 and only one can be defined -->
            - <xs:element name="Retencion" maxOccurs="unbounded"> <!-- belongs to Retenciones, at least 1 or an unlimited amount of them must be defined -->
        - <xs:element name="Traslados" minOccurs="0"> <!-- belongs to Impuestos, can be omitted or 1 and only one can be defined -->
            - <xs:element name="Traslado" maxOccurs="unbounded"> <!-- belongs to Traslados, at least 1 or an unlimited amount of them must be defined -->
